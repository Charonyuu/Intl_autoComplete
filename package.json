{
  "name": "intl-snippets",
  "displayName": "intl_autoComplete",
  "description": "react-intl snippet ",
  "version": "1.0.2",
  "publisher": "charonyuu",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.85.1"
  },
  "repository": {},
  "categories": [
    "Snippets"
  ],
  "main": "./index.js",
  "activationEvents": [
    "onCommand:extension.findKey"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/my-snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/my-snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/my-snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/my-snippets.json"
      }
    ],
    "commands": [
      {
        "command": "extension.findKey",
        "title": "Find Key for Selected Value in en.json"
      }
    ],
    "languages": [
      {
        "id": "javascript",
        "extensions": [
          ".js"
        ]
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "extension.findKey",
          "when": "editorTextFocus"
        }
      ]
    },
    "keybindings": [
      {
        "command": "extension.findKey",
        "key": "alt+k",
        "mac": "alt+k",
        "when": "editorTextFocus"
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm version patch && git add package.json"
    }
  },
  "scripts": {
    "build": "vsce package"
  },
  "license": "MIT",
  "devDependencies": {
    "husky": "^9.0.11"
  }
}
